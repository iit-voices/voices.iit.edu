}
</style>
<script type="text/javascript">
$(document).ready(function(){
    var map_id_vothL155 = new OpenLayers.Map('map_vothL155', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL155.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL155.addLayer(markers);
map_id_vothL155.zoomToMaxExtent();

                        var lat_5 = 49.283300;
                        var lon_5 = 19.900000;
                        var vothL155 = new OpenLayers.LonLat(lon_5, lat_5);
            vothL155_icon = new OpenLayers.Marker(vothL155);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL155);
markers.addMarker(vothL155_icon);
    addMarker(vothL155, popupClass, "", true, false, false);
    map_id_vothL155.setCenter(vothL155, 4); 
        
});
</script>
