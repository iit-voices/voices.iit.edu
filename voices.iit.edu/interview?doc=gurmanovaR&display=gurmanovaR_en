width:250px !important;
height:250px !important;
}
</style>
<script type="text/javascript">
$(document).ready(function(){
    var map_id_vothL513 = new OpenLayers.Map('map_vothL513', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL513.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL513.addLayer(markers);
map_id_vothL513.zoomToMaxExtent();

                        var lat_21 = 53.850000;
                        var lon_21 = 14.633300;
                        var vothL513 = new OpenLayers.LonLat(lon_21, lat_21);
            vothL513_icon = new OpenLayers.Marker(vothL513);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL513);
markers.addMarker(vothL513_icon);
    addMarker(vothL513, popupClass, "", true, false, false);
    map_id_vothL513.setCenter(vothL513, 4); 
        
});
</script>
