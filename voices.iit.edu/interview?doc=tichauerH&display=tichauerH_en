         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL92.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL92.addLayer(markers);
map_id_vothL92.zoomToMaxExtent();

                        var lat_15 = 51.300000;
                        var lon_15 = 22.516700;
                        var vothL92 = new OpenLayers.LonLat(lon_15, lat_15);
            vothL92_icon = new OpenLayers.Marker(vothL92);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL92);
markers.addMarker(vothL92_icon);
    addMarker(vothL92, popupClass, "", true, false, false);
    map_id_vothL92.setCenter(vothL92, 4); 
        
});
</script>
</div>
