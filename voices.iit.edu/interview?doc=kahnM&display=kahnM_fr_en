 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL336.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL336.addLayer(markers);
map_id_vothL336.zoomToMaxExtent();

                        var lat_9 = 36.776300;
                        var lon_9 = 3.058500;
                        var vothL336 = new OpenLayers.LonLat(lon_9, lat_9);
            vothL336_icon = new OpenLayers.Marker(vothL336);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL336);
markers.addMarker(vothL336_icon);
    addMarker(vothL336, popupClass, "", true, false, false);
    map_id_vothL336.setCenter(vothL336, 4); 
        
});
</script>
</div>
