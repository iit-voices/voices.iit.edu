                                <div class="map widget vothL29">
    <p>
        <b>
            <a href="#close" class="hide">
                [x] click to close
            </a>
        </b>
    </p>
    <div id="map_vothL29" style="width: 250px; height: 250px;"></div>
            <p class="municipality">
            M&#195;nchen, Germany
            <br />
                    </p>
    <style type="text/css">
.olLayerDiv{
width:250px !important;
height:250px !important;
}
</style>
<script type="text/javascript">
$(document).ready(function(){
    var map_id_vothL29 = new OpenLayers.Map('map_vothL29', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
