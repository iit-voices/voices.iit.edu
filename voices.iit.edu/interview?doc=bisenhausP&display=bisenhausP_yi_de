            <br />
                    </p>
    <style type="text/css">
.olLayerDiv{
width:250px !important;
height:250px !important;
}
</style>
<script type="text/javascript">
$(document).ready(function(){
    var map_id_vothL77 = new OpenLayers.Map('map_vothL77', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL77.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL77.addLayer(markers);
map_id_vothL77.zoomToMaxExtent();

                        var lat_9 = 50.550000;
                        var lon_9 = 21.116700;
                        var vothL77 = new OpenLayers.LonLat(lon_9, lat_9);
            vothL77_icon = new OpenLayers.Marker(vothL77);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL77);
markers.addMarker(vothL77_icon);
    addMarker(vothL77, popupClass, "", true, false, false);
    map_id_vothL77.setCenter(vothL77, 4); 
        
});
</script>
