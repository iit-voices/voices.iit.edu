                        <p class="placename">
                Kielce Ghetto
            </p>
                                    <p class="municipality">
                <strong>Location:</strong>
                Kielce, Poland<br />
                            </p>
                            <div>
                Established on March 31, 1941 for area Jews, this ghetto was largely liquidated in August 1942 when 20,000-21,000 inhabitants were sent to the extermination camp at Treblinka.
            </div>
                            <style type="text/css">
.olLayerDiv{
width:250px !important;
height:250px !important;
}
</style>
<script type="text/javascript">
$(document).ready(function(){
    var map_id_vothC52 = new OpenLayers.Map('map_vothC52', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothC52.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothC52.addLayer(markers);
map_id_vothC52.zoomToMaxExtent();

                        var lat_1 = 50.850000;
                        var lon_1 = 20.650000;
                        var vothC52 = new OpenLayers.LonLat(lon_1, lat_1);
            vothC52_icon = new OpenLayers.Marker(vothC52);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothC52);
markers.addMarker(vothC52_icon);
    addMarker(vothC52, popupClass, "", true, false, false);
    map_id_vothC52.setCenter(vothC52, 4); 
        
});
</script>
