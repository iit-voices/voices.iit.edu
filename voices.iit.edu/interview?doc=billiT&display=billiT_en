<script type="text/javascript">
$(document).ready(function(){
    var map_id_vothL29 = new OpenLayers.Map('map_vothL29', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL29.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL29.addLayer(markers);
map_id_vothL29.zoomToMaxExtent();

                        var lat_1 = 48.133300;
                        var lon_1 = 11.583300;
                        var vothL29 = new OpenLayers.LonLat(lon_1, lat_1);
            vothL29_icon = new OpenLayers.Marker(vothL29);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL29);
markers.addMarker(vothL29_icon);
    addMarker(vothL29, popupClass, "", true, false, false);
    map_id_vothL29.setCenter(vothL29, 4); 
        
});
</script>
