            <a href="#close" class="hide">
                [x] click to close
            </a>
        </b>
    </p>
    <div id="map_vothL374" style="width: 250px; height: 250px;"></div>
            <p class="municipality">
            Ni&#195;vre department, France
            <br />
                    </p>
    <style type="text/css">
.olLayerDiv{
width:250px !important;
height:250px !important;
}
</style>
<script type="text/javascript">
$(document).ready(function(){
    var map_id_vothL374 = new OpenLayers.Map('map_vothL374', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL374.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL374.addLayer(markers);
map_id_vothL374.zoomToMaxExtent();

                        var lat_15 = 47.083333;
                        var lon_15 = 3.500000;
                        var vothL374 = new OpenLayers.LonLat(lon_15, lat_15);
            vothL374_icon = new OpenLayers.Marker(vothL374);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL374);
markers.addMarker(vothL374_icon);
    addMarker(vothL374, popupClass, "", true, false, false);
    map_id_vothL374.setCenter(vothL374, 4); 
        
});
</script>
</div>
