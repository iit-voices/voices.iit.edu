    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothC27.addLayer(markers);
map_id_vothC27.zoomToMaxExtent();

                        var lat_1 = 48.933300;
                        var lon_1 = 2.433300;
                        var vothC27 = new OpenLayers.LonLat(lon_1, lat_1);
            vothC27_icon = new OpenLayers.Marker(vothC27);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothC27);
markers.addMarker(vothC27_icon);
    addMarker(vothC27, popupClass, "", true, false, false);
    map_id_vothC27.setCenter(vothC27, 4); 
        
});
