   map_id_vothL282.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL282.addLayer(markers);
map_id_vothL282.zoomToMaxExtent();

                        var lat_4 = 41.416700;
                        var lon_4 = 2.166700;
                        var vothL282 = new OpenLayers.LonLat(lon_4, lat_4);
            vothL282_icon = new OpenLayers.Marker(vothL282);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL282);
markers.addMarker(vothL282_icon);
    addMarker(vothL282, popupClass, "", true, false, false);
    map_id_vothL282.setCenter(vothL282, 4); 
        
});
</script>
</div>
