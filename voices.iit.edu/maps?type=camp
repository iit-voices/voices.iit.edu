                        vothL207 = new OpenLayers.LonLat(lon_99, lat_99);
                                    vothL207_icon = new OpenLayers.Marker(vothL207)
            markers.addMarker(vothL207_icon);
            var popupContentHTML = "";
                                            popupContentHTML += '<p class="placename">Weidenberg</p>';
                                                                popupContentHTML += '<p class="municipality"><strong>Location:</strong> Weidenberg, Germany<br /></p>';
                                                popupContentHTML += '<div>No information on this camp could be found.</div>';
                                                                popupContentHTML += '<p><a href="search_results?filter_by=internment_location_id&filter_value=vothC119">Read interviews with persons who were in this camp</a></p>';
                        addMarker(vothL207, popupClass, popupContentHTML, true, true);
                                        var lat_100 = 50.000000;
                        var lon_100 = 20.033300;
                        vothL85 = new OpenLayers.LonLat(lon_100, lat_100);
                                    vothL85_icon = new OpenLayers.Marker(vothL85)
            markers.addMarker(vothL85_icon);
            var popupContentHTML = "";
                                            popupContentHTML += '<p class="placename">Wieliczka</p>';
                                                                popupContentHTML += '<p class="municipality"><strong>Location:</strong> Wieliczka, Poland<br /></p>';
                                                popupContentHTML += '<div>This subcamp of the Krakau-Plaszow concentration camp was created in the spring of 1944 and produced Heinkel aircraft.  The camp was abandoned in September 1944.</div>';
                                                                popupContentHTML += '<p><a href="search_results?filter_by=internment_location_id&filter_value=vothC121">Read interviews with persons who were in this camp</a></p>';
                        addMarker(vothL85, popupClass, popupContentHTML, true, true);
                                        var lat_101 = 53.533300;
                        var lon_101 = 8.116700;
                        vothL208 = new OpenLayers.LonLat(lon_101, lat_101);
                                    vothL208_icon = new OpenLayers.Marker(vothL208)
            markers.addMarker(vothL208_icon);
            var popupContentHTML = "";
                                            popupContentHTML += '<p class="placename">Wilhelmshaven<br /><span class="alt">(Alter Bant Weg)</span></p>';
                                                popupContentHTML += '<p class="subcamp_of"><strong>Subcamp of: </strong>Neuengamme</p>';
                                                popupContentHTML += '<p class="municipality"><strong>Location:</strong> Wilhelmshaven, Germany<br /></p>';
                                                popupContentHTML += '<div>Alter Bant Weg (No. 1582 Wilhelmshaven) was a subcamp of the Neuengamme concentration camp.  Two-thirds of the town&#39;s buildings were destroyed during bombing by the Allies of World War II.</div>';
                                                                popupContentHTML += '<p><a href="search_results?filter_by=internment_location_id&filter_value=vothC122">Read interviews with persons who were in this camp</a></p>';
                        addMarker(vothL208, popupClass, popupContentHTML, true, true);
                                        var lat_102 = 50.683333;
                        var lon_102 = 16.366667;
                        vothL395 = new OpenLayers.LonLat(lon_102, lat_102);
                        bounds.extend(vothL395);            vothL395_icon = new OpenLayers.Marker(vothL395)
            markers.addMarker(vothL395_icon);
            var popupContentHTML = "";
                                            popupContentHTML += '<p class="placename">W&#195;stegiersdorf</p>';
                                                popupContentHTML += '<p class="subcamp_of"><strong>Subcamp of: </strong>Gross-Rosen</p>';
                                                popupContentHTML += '<p class="municipality"><strong>Location:</strong> Głuszyca, Poland<br /></p>';
                                                popupContentHTML += '<div>Part of the Riese/Kaltwasser complex in the Polish Owl Mountains.</div>';
                                                                popupContentHTML += '<p><a href="search_results?filter_by=internment_location_id&filter_value=vothC147">Read interviews with persons who were in this camp</a></p>';
                        addMarker(vothL395, popupClass, popupContentHTML, true, true);
                    
    map.setCenter(vothL63, 4);
});
</script></head>
