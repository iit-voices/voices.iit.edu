         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL516.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL516.addLayer(markers);
map_id_vothL516.zoomToMaxExtent();

                        var lat_1 = 49.700000;
                        var lon_1 = 4.950000;
                        var vothL516 = new OpenLayers.LonLat(lon_1, lat_1);
            vothL516_icon = new OpenLayers.Marker(vothL516);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL516);
markers.addMarker(vothL516_icon);
    addMarker(vothL516, popupClass, "", true, false, false);
    map_id_vothL516.setCenter(vothL516, 4); 
        
});
</script>
