width:250px !important;
height:250px !important;
}
</style>
<script type="text/javascript">
$(document).ready(function(){
    var map_id_vothL356 = new OpenLayers.Map('map_vothL356', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL356.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL356.addLayer(markers);
map_id_vothL356.zoomToMaxExtent();

                        var lat_18 = 48.285500;
                        var lon_18 = 13.080000;
                        var vothL356 = new OpenLayers.LonLat(lon_18, lat_18);
            vothL356_icon = new OpenLayers.Marker(vothL356);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL356);
markers.addMarker(vothL356_icon);
    addMarker(vothL356, popupClass, "", true, false, false);
    map_id_vothL356.setCenter(vothL356, 4); 
        
});
</script>
