<script type="text/javascript">
$(document).ready(function(){
    var map_id_vothL467 = new OpenLayers.Map('map_vothL467', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL467.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL467.addLayer(markers);
map_id_vothL467.zoomToMaxExtent();

                        var lat_5 = 52.166700;
                        var lon_5 = 22.300000;
                        var vothL467 = new OpenLayers.LonLat(lon_5, lat_5);
            vothL467_icon = new OpenLayers.Marker(vothL467);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL467);
markers.addMarker(vothL467_icon);
    addMarker(vothL467, popupClass, "", true, false, false);
    map_id_vothL467.setCenter(vothL467, 4); 
        
});
</script>
</div>
