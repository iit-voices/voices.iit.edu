   map_id_vothL535.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL535.addLayer(markers);
map_id_vothL535.zoomToMaxExtent();

                        var lat_27 = 48.250000;
                        var lon_27 = 12.433300;
                        var vothL535 = new OpenLayers.LonLat(lon_27, lat_27);
            vothL535_icon = new OpenLayers.Marker(vothL535);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL535);
markers.addMarker(vothL535_icon);
    addMarker(vothL535, popupClass, "", true, false, false);
    map_id_vothL535.setCenter(vothL535, 4); 
        
});
</script>
