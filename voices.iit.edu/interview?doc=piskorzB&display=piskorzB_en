                        <p class="placename">
                Dachau
            </p>
                            <p class="subcamp_of">
                <strong>Subcamp of: </strong>
                Auschwitz
            </p>
                            <p class="municipality">
                <strong>Location:</strong>
                Dachau, Germany<br />
                            </p>
                            <div>
                The first concentration camp opened in Germany (March 1933), located on the grounds of an abandoned munitions factory near the medieval town of Dachau, about 16 km (10 miles) northwest of Munich. In total, over 200,000 prisoners from more than 30 countries were housed in Dachau.
            </div>
                            <style type="text/css">
.olLayerDiv{
width:250px !important;
height:250px !important;
}
</style>
<script type="text/javascript">
$(document).ready(function(){
    var map_id_vothC25 = new OpenLayers.Map('map_vothC25', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothC25.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothC25.addLayer(markers);
map_id_vothC25.zoomToMaxExtent();

                        var lat_7 = 48.250000;
                        var lon_7 = 11.433300;
                        var vothC25 = new OpenLayers.LonLat(lon_7, lat_7);
            vothC25_icon = new OpenLayers.Marker(vothC25);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothC25);
markers.addMarker(vothC25_icon);
    addMarker(vothC25, popupClass, "", true, false, false);
    map_id_vothC25.setCenter(vothC25, 4); 
        
});
</script>
