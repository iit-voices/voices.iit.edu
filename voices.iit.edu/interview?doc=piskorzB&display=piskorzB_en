    <div id="map_vothC117" style="width: 250px; height: 250px;"></div>
                        <p class="placename">
                Warsaw Ghetto
            </p>
                                    <p class="municipality">
                <strong>Location:</strong>
                Warsaw, Poland<br />
                            </p>
                            <div>
                Established on October 16, 1940 with around 400,000 people (about 30% of the population of Warsaw but housed in an area covering only 2.4% of the city).  Fighting broke out in January 1943 and ended on April 23; survivors were either killed or deported to concentration and death camps.
            </div>
                            <style type="text/css">
.olLayerDiv{
width:250px !important;
height:250px !important;
}
</style>
<script type="text/javascript">
$(document).ready(function(){
    var map_id_vothC117 = new OpenLayers.Map('map_vothC117', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothC117.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothC117.addLayer(markers);
map_id_vothC117.zoomToMaxExtent();

                        var lat_1 = 52.250000;
                        var lon_1 = 21.000000;
                        var vothC117 = new OpenLayers.LonLat(lon_1, lat_1);
            vothC117_icon = new OpenLayers.Marker(vothC117);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothC117);
markers.addMarker(vothC117_icon);
    addMarker(vothC117, popupClass, "", true, false, false);
    map_id_vothC117.setCenter(vothC117, 4); 
        
});
</script>
