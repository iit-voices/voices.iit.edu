 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL291.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL291.addLayer(markers);
map_id_vothL291.zoomToMaxExtent();

                        var lat_19 = 45.800000;
                        var lon_19 = 9.083300;
                        var vothL291 = new OpenLayers.LonLat(lon_19, lat_19);
            vothL291_icon = new OpenLayers.Marker(vothL291);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL291);
markers.addMarker(vothL291_icon);
    addMarker(vothL291, popupClass, "", true, false, false);
    map_id_vothL291.setCenter(vothL291, 4); 
        
