         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL68.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL68.addLayer(markers);
map_id_vothL68.zoomToMaxExtent();

                        var lat_6 = 40.633300;
                        var lon_6 = 22.933300;
                        var vothL68 = new OpenLayers.LonLat(lon_6, lat_6);
            vothL68_icon = new OpenLayers.Marker(vothL68);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL68);
markers.addMarker(vothL68_icon);
    addMarker(vothL68, popupClass, "", true, false, false);
    map_id_vothL68.setCenter(vothL68, 4); 
        
});
</script>
</div>
