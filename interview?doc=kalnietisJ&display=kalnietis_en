     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL129.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL129.addLayer(markers);
map_id_vothL129.zoomToMaxExtent();

                        var lat_3 = 56.883300;
                        var lon_3 = 24.133300;
                        var vothL129 = new OpenLayers.LonLat(lon_3, lat_3);
            vothL129_icon = new OpenLayers.Marker(vothL129);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL129);
markers.addMarker(vothL129_icon);
    addMarker(vothL129, popupClass, "", true, false, false);
    map_id_vothL129.setCenter(vothL129, 4); 
        
});
</script>
</div>
