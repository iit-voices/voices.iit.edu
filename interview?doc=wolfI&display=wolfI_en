    <div id="map_vothL589" style="width: 250px; height: 250px;"></div>
            <p class="municipality">
            Podvolochisk, Ukraine
            <br />
                    </p>
    <style type="text/css">
.olLayerDiv{
width:250px !important;
height:250px !important;
}
</style>
<script type="text/javascript">
$(document).ready(function(){
    var map_id_vothL589 = new OpenLayers.Map('map_vothL589', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL589.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL589.addLayer(markers);
map_id_vothL589.zoomToMaxExtent();

                        var lat_8 = 49.541000;
                        var lon_8 = 26.158000;
                        var vothL589 = new OpenLayers.LonLat(lon_8, lat_8);
            vothL589_icon = new OpenLayers.Marker(vothL589);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL589);
markers.addMarker(vothL589_icon);
    addMarker(vothL589, popupClass, "", true, false, false);
    map_id_vothL589.setCenter(vothL589, 4); 
        
});
</script>
</div>
