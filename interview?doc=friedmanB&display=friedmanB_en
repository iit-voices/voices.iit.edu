                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL502.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL502.addLayer(markers);
map_id_vothL502.zoomToMaxExtent();

                        var lat_13 = 48.583300;
                        var lon_13 = 19.166700;
                        var vothL502 = new OpenLayers.LonLat(lon_13, lat_13);
            vothL502_icon = new OpenLayers.Marker(vothL502);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL502);
markers.addMarker(vothL502_icon);
    addMarker(vothL502, popupClass, "", true, false, false);
    map_id_vothL502.setCenter(vothL502, 4); 
        
});
</script>
</div>
