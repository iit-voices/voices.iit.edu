.olLayerDiv{
width:250px !important;
height:250px !important;
}
</style>
<script type="text/javascript">
$(document).ready(function(){
    var map_id_vothC69 = new OpenLayers.Map('map_vothC69', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothC69.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothC69.addLayer(markers);
map_id_vothC69.zoomToMaxExtent();

                        var lat_1 = 51.300000;
                        var lon_1 = 22.516700;
                        var vothC69 = new OpenLayers.LonLat(lon_1, lat_1);
            vothC69_icon = new OpenLayers.Marker(vothC69);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothC69);
markers.addMarker(vothC69_icon);
    addMarker(vothC69, popupClass, "", true, false, false);
    map_id_vothC69.setCenter(vothC69, 4); 
        
});
</script>
</div>
