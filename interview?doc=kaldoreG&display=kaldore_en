         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL138.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL138.addLayer(markers);
map_id_vothL138.zoomToMaxExtent();

                        var lat_20 = 49.783300;
                        var lon_20 = 22.783300;
                        var vothL138 = new OpenLayers.LonLat(lon_20, lat_20);
            vothL138_icon = new OpenLayers.Marker(vothL138);
            
