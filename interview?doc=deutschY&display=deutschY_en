    var map_id_vothL278 = new OpenLayers.Map('map_vothL278', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL278.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL278.addLayer(markers);
map_id_vothL278.zoomToMaxExtent();

                        var lat_12 = 47.550000;
                        var lon_12 = 7.600000;
                        var vothL278 = new OpenLayers.LonLat(lon_12, lat_12);
            vothL278_icon = new OpenLayers.Marker(vothL278);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL278);
markers.addMarker(vothL278_icon);
    addMarker(vothL278, popupClass, "", true, false, false);
    map_id_vothL278.setCenter(vothL278, 4); 
        
});
</script>
</div>
