                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL455.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL455.addLayer(markers);
map_id_vothL455.zoomToMaxExtent();

                        var lat_13 = 48.216700;
                        var lon_13 = 14.466700;
                        var vothL455 = new OpenLayers.LonLat(lon_13, lat_13);
            vothL455_icon = new OpenLayers.Marker(vothL455);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL455);
markers.addMarker(vothL455_icon);
    addMarker(vothL455, popupClass, "", true, false, false);
    map_id_vothL455.setCenter(vothL455, 4); 
        
});
</script>
</div>
                                <div class="map widget vothL5">
    <p>
