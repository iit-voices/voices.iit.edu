<script type="text/javascript">
$(document).ready(function(){
    var map_id_vothC135 = new OpenLayers.Map('map_vothC135', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothC135.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothC135.addLayer(markers);
map_id_vothC135.zoomToMaxExtent();

                        var lat_1 = 47.966700;
                        var lon_1 = 22.333300;
                        var vothC135 = new OpenLayers.LonLat(lon_1, lat_1);
            vothC135_icon = new OpenLayers.Marker(vothC135);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothC135);
markers.addMarker(vothC135_icon);
    addMarker(vothC135, popupClass, "", true, false, false);
    map_id_vothC135.setCenter(vothC135, 4); 
        
});
</script>
