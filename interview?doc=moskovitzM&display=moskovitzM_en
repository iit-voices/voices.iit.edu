<script type="text/javascript">
$(document).ready(function(){
    var map_id_vothL545 = new OpenLayers.Map('map_vothL545', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL545.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL545.addLayer(markers);
map_id_vothL545.zoomToMaxExtent();

                        var lat_14 = 47.950000;
                        var lon_14 = 8.733300;
                        var vothL545 = new OpenLayers.LonLat(lon_14, lat_14);
            vothL545_icon = new OpenLayers.Marker(vothL545);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL545);
markers.addMarker(vothL545_icon);
    addMarker(vothL545, popupClass, "", true, false, false);
    map_id_vothL545.setCenter(vothL545, 4); 
        
});
</script>
</div>
