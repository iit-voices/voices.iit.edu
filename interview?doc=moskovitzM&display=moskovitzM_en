/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothC101.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothC101.addLayer(markers);
map_id_vothC101.zoomToMaxExtent();

                        var lat_1 = 47.933300;
                        var lon_1 = 23.883300;
                        var vothC101 = new OpenLayers.LonLat(lon_1, lat_1);
            vothC101_icon = new OpenLayers.Marker(vothC101);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothC101);
markers.addMarker(vothC101_icon);
    addMarker(vothC101, popupClass, "", true, false, false);
    map_id_vothC101.setCenter(vothC101, 4); 
        
});
</script>
