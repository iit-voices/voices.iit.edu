                        var vothL332 = new OpenLayers.LonLat(lon_5, lat_5);
            vothL332_icon = new OpenLayers.Marker(vothL332);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL332);
markers.addMarker(vothL332_icon);
    addMarker(vothL332, popupClass, "", true, false, false);
    map_id_vothL332.setCenter(vothL332, 4); 
        
});
</script>
</div>
