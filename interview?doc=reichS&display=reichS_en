    var map_id_vothL616 = new OpenLayers.Map('map_vothL616', {controls: []});
/*    gphy = new OpenLayers.Layer.Google(
                  "Google Physical",
                  {type: G_PHYSICAL_MAP,
                  'sphericalMercator': true,
     'maxExtent': new OpenLayers.Bounds(-20037508.34,-20037508.34,20037508.34,20037508.34)
}
               );
*/
 var ol_wms = new OpenLayers.Layer.WMS(
         "OpenLayers WMS",
         "http://labs.metacarta.com/wms/vmap0",
         {layers: 'basic'}
     );
   map_id_vothL616.addLayer(ol_wms);
    markers = new OpenLayers.Layer.Markers("Markers");
    map_id_vothL616.addLayer(markers);
map_id_vothL616.zoomToMaxExtent();

                        var lat_23 = 47.284508;
                        var lon_23 = 8.549544;
                        var vothL616 = new OpenLayers.LonLat(lon_23, lat_23);
            vothL616_icon = new OpenLayers.Marker(vothL616);
            
var bounds = new OpenLayers.Bounds();
bounds.extend(vothL616);
markers.addMarker(vothL616_icon);
    addMarker(vothL616, popupClass, "", true, false, false);
    map_id_vothL616.setCenter(vothL616, 4); 
        
});
</script>
</div>
